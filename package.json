{
  "name": "@adrianlafond/geom",
  "version": "1.2.9",
  "description": "Geometrical and mathematical helper utils",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "start": "npm run clean && \"npm run test:watch\"",
    "clean": "shx rm -rf dist",
    "build": "npm run clean && tsc ./src/index.ts --outDir ./dist --declaration",
    "docs": "typedoc --plugin typedoc-plugin-markdown",
    "test:watch": "jest --watchAll --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrianlafond/geom.git"
  },
  "keywords": [
    "geometry",
    "math",
    "point"
  ],
  "author": "Adrian Lafond <mail@adrianlafond.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adrianlafond/geom/issues"
  },
  "homepage": "https://github.com/adrianlafond/geom#readme",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.8",
    "jest": "^29.6.2",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.4",
    "typescript": "^5.1.6"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
